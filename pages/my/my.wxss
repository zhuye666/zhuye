.topimg{
    position: relative;
    text-align: center;
  }
  .topimg>image{
    width: 90%;
    height: 250rpx;
    border-radius: 20rpx;
  }
  .topimg .toptitle{
    position: absolute;
    color: white;
    top:40rpx;
    left: 75rpx;
    font-size: 28rpx;
    text-align: left;
  }
  .topimg .toptitle>view{
    font-size: 23rpx;
  }
  .topimg .toptitle>view:first-child{         /* >:直接子主键 */
    font-size: 34rpx;
    font-weight: 900;
    margin-bottom: 20rpx;
  }
  .topimg .toptitle>view:last-child{
    margin-top: 20rpx;
    width: 100rpx;
    height: 45rpx;
    border: 1rpx solid white;
    text-align: center;
    line-height: 45rpx;
    border-radius: 25rpx;
  }
  .barlist{
    width: 90%;
    display: flex;
    margin: auto;
    justify-content: space-between;
  }
  .barlist>view>view:first-child{
    position: relative;
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    margin: auto;
    margin-bottom: 20rpx;
    margin-top: 30rpx;
  }
  .barlist>view>view:first-child>image{
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    width: 35rpx;
    height: 35rpx;
  }
  .barlist .yellow{
    background-color: rgb(246,176,89);
  }
  .barlist>view:nth-child(2)>view:first-child{
    background-color: rgb(143,106,255);
  }
  .barlist>view:nth-child(3)>view:first-child{
    background-color: rgb(236,30,113);
  }
  .barlist>view:nth-child(4)>view:first-child{
    background-color: rgb(3,227,145);
  }
  .barlist>view:nth-child(5)>view:first-child{
    background-color: rgb(30,157,255);
  }
  .barlist>view>view:last-child{
    font-size: 26rpx;
    text-align: center;
  }
  
  .things{
    width: 90%;
    height: 250rpx;
    margin: auto;
    display: flex;
    justify-content: space-between;
    margin-top: 40rpx;
    position: relative;
  }
  .things .left{
    position: relative;
    width: 48%;
    height: 200rpx;
    /* background-color: #FF8260; */
    background: linear-gradient(to right,#FEB27A,#FF805F);
    border-radius: 20rpx;
  }
  .things .left>image{
    width: 220rpx;
    height: 200rpx;
    position: absolute;
    bottom: 0;
  }
  .things .left .midtitle{
    position: absolute;
    right: 26rpx;
    top: 60rpx;
    color: white;
  }
  .things .left .midtitle>view:first-child{
    font-size: 28rpx;
    margin-bottom: 10rpx;
  }
  .things .left .midtitle>view:last-child{
    font-size: 23rpx;
  }
  .things .right{
    position: relative;
    width: 48%;
    height: 200rpx;
    background: linear-gradient(to right,#95F9DA,#68D0E4);  /* 线性渐变色 */
    border-radius: 20rpx;
  }
  .things .right>image{
    width: 230rpx;
    height: 200rpx;
    position: absolute;
    right: -13rpx;
  }
  .things .right .midtitle{
    position: absolute;
    left: 25rpx;
    top: 60rpx;
    color: white;
  }
  .things .right .midtitle>view:first-child{
    font-size: 28rpx;
    margin-bottom: 10rpx;
  }
  .things .right .midtitle>view:last-child{
    font-size: 23rpx;
  }
  
  .flexbox{
    display: flex;
  }
  .runtime{
    width: 90%;
    margin: auto;
    align-items: center;
    margin-bottom: 70rpx;
  }
  .runtime>view:first-child{
    width : 50rpx;
    height: 50rpx;
    border-radius: 50%;
    background-color: #00AD7B;
    margin-right: 20rpx;
    justify-content: center;
    align-items: center;
  }
  .runtime>view:first-child>image{
    width: 35rpx;
    height: 35rpx;
  }
  .runtime>view:last-child{
    font-size: 26rpx;
  }
  
