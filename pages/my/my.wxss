.topimg{
    position: relative;
    text-align: center;
  }
  .topimg>image{
    width: 90%;
    height: 250rpx;
    border-radius: 20rpx;
  }
  .topimg .toptitle{
    position: absolute;
    color: white;
    top:40rpx;
    left: 75rpx;
    font-size: 28rpx;
    text-align: left;
  }
  .topimg .toptitle>view{
    font-size: 23rpx;
  }
  .topimg .toptitle>view:first-child{         /* >:直接子主键 */
    font-size: 34rpx;
    font-weight: 900;
    margin-bottom: 20rpx;
  }
  .topimg .toptitle>view:last-child{
    margin-top: 20rpx;
    width: 100rpx;
    height: 45rpx;
    border: 1rpx solid white;
    text-align: center;
    line-height: 45rpx;
    border-radius: 25rpx;
  }
  .barlist{
    width: 90%;
    display: flex;
    margin: auto;
    justify-content: space-between;
  }
  .barlist>view>view:first-child{
    position: relative;
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    margin: auto;
    margin-bottom: 20rpx;
    margin-top: 30rpx;
  }
  .barlist>view>view:first-child>image{
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    width: 35rpx;
    height: 35rpx;
  }
  .barlist .yellow{
    background-color: rgb(246,176,89);
  }
  .barlist>view:nth-child(2)>view:first-child{
    background-color: rgb(143,106,255);
  }
  .barlist>view:nth-child(3)>view:first-child{
    background-color: rgb(236,30,113);
  }
  .barlist>view:nth-child(4)>view:first-child{
    background-color: rgb(3,227,145);
  }
  .barlist>view:nth-child(5)>view:first-child{
    background-color: rgb(30,157,255);
  }
  .barlist>view>view:last-child{
    font-size: 26rpx;
    text-align: center;
  }
  
