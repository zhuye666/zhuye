/* pages/message/message.wxss */
/* 搜索栏 */
.w90{
    width: 90%;
    height: auto;
    display: flex;
    justify-content: center;
    align-items: center; 
  }
  .Search{
    display: flex;
    justify-content: center;
    align-items: center; 
    height: 70rpx;
    box-shadow: 2px 2px 2px 1px rgba(139, 139, 139, 0.2);
  }
  .Search .w90>view{
    width: 300rpx;
    height: 50rpx;
    display: flex;
    align-items: center;
    padding-left: 10rpx;
    box-shadow: 0px 2px 2px 1px rgba(139, 139, 139, 0.1);
    border-radius: 20rpx;
    margin-left:35rpx;
  }
  .Search .w90>view>image{
    width: 45rpx;
    height: 35rpx;
  }
  .Search .w90>view>input{
    width: 190px;
    height: 20px;
    font-size: 18rpx;
    padding-left:10rpx;
    border: none; 
  }
  .Search .w90>image{
    width: 50rpx;
    height: 50rpx;
    margin-left:35rpx;
}
.Search .w90>text{
  font-size: 33rpx;
  font-weight: 900;
  margin-right: 20rpx;
}
